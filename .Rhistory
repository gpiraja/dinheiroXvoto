plotEE <- function(x){
title <- paste("Heating Load vs", x, "\n conditioned on OverallHeight and Orientation")
ggplot(eeframe, aes_string(x, "HeatingLoad")) +
geom_point() +
facet_grid(OverallHeight ~ Orientation) +
ggtitle(title) +
stat_smooth(method = "lm")
}
lapply(plotCols, plotEE)
