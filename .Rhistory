eeHist <- function(x) {
title <- paste("Histogram of", x, "conditioned on OverallHeight")
ggplot(eeframe, aes_string(x)) +
geom_histogram(aes(y = ..density..)) +
facet_grid(. ~ OverallHeight) +
ggtitle(title) +
geom_density()
}
lapply(plotCols4, eeHist)
